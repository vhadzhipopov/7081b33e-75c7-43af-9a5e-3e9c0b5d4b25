<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.sample.stockwatcher.client.ui.component">
    <!-- import for resources bundle -->
    <ui:with type="com.google.gwt.sample.stockwatcher.client.resource.ApplicationResources"
             field="res"/>

    <g:FlowPanel styleName="{res.style.currencyblock}">
        <g:InlineLabel styleName="{res.style.currencyText}" text="Test CURRENCY" ui:field="textBox"/>
        <c:ImageButton resource="{res.deleteIcon}" styleName="{res.style.sendButton}" text="Delete" ui:field="deleteButton"/>
    </g:FlowPanel>
</ui:UiBinder> 